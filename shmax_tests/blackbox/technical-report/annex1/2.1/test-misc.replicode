; Doc - Annex 1 - 2.1 - Syntax - Misc

; Replicode is case-sensitive.
a:(ent 1) [[SYNC_ONCE now 0 forever root nil]]
A:(ent 1) [[SYNC_ONCE now 0 forever root nil]]
; A:(ent 1) [[SYNC_ONCE now 0 forever root nil]] ; should fail


; | is not an operator.
pgm0:(pgm 
|[]
[]
   (ptn x: |[])
[]
   (= x nil)
   ; (| x nil) ; should fail
|[] 
1) |[]


; test different variable/label/member rules
ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789_:(ent 1) [[SYNC_ONCE now 0 forever root nil]]
; digit as first char -> should fail
;0ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789_:(ent 1) [[SYNC_ONCE now 0 forever root nil]]
; underscore as first char -> should fail. but doesnt!
_ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789_:(ent 1) [[SYNC_ONCE now 0 forever root nil]]

; the string cannot be equal to any of the symbols denoting atoms, built-in operators, built-in
; object types, built-in device functions, and built-in macros.
; TODO: all of these should fail, but do actually compile!
ipgm:(ent 1) [[SYNC_ONCE now 0 forever root nil]]
inj:(ent 1) [[SYNC_ONCE now 0 forever root nil]]
view:(ent 1) [[SYNC_ONCE now 0 forever root nil]]
SYNC_ONCE:(ent 1) [[SYNC_ONCE now 0 forever root nil]]


; Atoms of type character string are limited in length (1020 characters)
; and can be composed of any character.
pgm1:(pgm 
|[]
|[]
|[]
[]
   (prb [1 "print" "" |[]])
   (prb [1 "print" "character-string" |[]])
   (prb [1 "print" "1234567890" |[]])
   (prb [1 "print" "!@#$%^&*()_+[]{}|;:',.<>/?" |[]])
   (prb [1 "print" "\n\t\r" |[]])
   (prb [1 "print" "" |[]])
   (prb [1 "print" "" |[]])
1) |[]

; instantiate programs
ipgm0:(ipgm pgm0 |[] RUN_ONCE 50ms VOLATILE SILENT 1) [[SYNC_ONCE now 0 1 stdin nil 1]]
ipgm1:(ipgm pgm1 |[] RUN_ONCE 50ms VOLATILE SILENT 1) [[SYNC_ONCE now 0 1 stdin nil 1]]